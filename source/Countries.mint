  /* Return country information from country code */
  fun all : Array(Countries.Country) {
    Array.map(
      COUNTRIES,
      (item : Tuple(String, String, String)) { Countries.Country(item[0], item[1], item[2]) })
  }

  /* Return country information from country code */
  fun fromCode (code : String) : Maybe(Countries.Country) {
    let country =
      Array.find(COUNTRIES, (item : Tuple(String, String, String)) : Bool { code == item[1] })

    case country {
      Maybe::Just(item) =>
        Maybe::Just(Countries.Country(item[0], item[1], item[2]))

      Maybe::Nothing =>
        Maybe::Nothing
    }
  }

  /* Returns states filtered by country code */
  fun states (code : String) : Array(Countries.State) {
    STATES
    |> Array.select((item : Tuple(String, String, String)) { code == item[0] })
    |> Array.map((item : Tuple(String, String, String)) { Countries.State(item[1], item[2]) })
  }
}
